<?xml version="1.0" encoding="utf-8"?>

<!--DEFINIR PATRON PARA TABLE LAYOUT-->

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TableRow>
            <TextView
                android:padding="10dp"
                android:id="@+id/textViewClaveLista"
                android:text="{Clave}"/>
            <TextView
                android:padding="10dp"
                android:id="@+id/textPresentacionLista"
                android:text="{Presentacion}"/>
            <TextView
                android:padding="10dp"
                android:id="@+id/textViewCantidadLista"
                android:text="{Cantidad}"/>
            <TextView
                android:padding="10dp"
                android:id="@+id/textViewLoteLista"
                android:text="{Lote}"/>
            <TextView
                android:padding="10dp"
                android:id="@+id/textViewFCaducidadLista"
                android:text="{F_Caducidad}"/>
        </TableRow>
    </TableLayout>
</RelativeLayout>